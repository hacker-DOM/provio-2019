{
  "name": "provio",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:file": "npx babel -d dist-file -f",
    "start:file": "npx babel-node --extensions \".ts\"",
    "start": "npx babel-node src/index.ts --extensions \".ts\"",
    "start:watch": "npx nodemon --exec \"npm run start\"",
    "build": "rm -rf dist && npx babel src --extensions \".ts\" -d dist --source-maps",
    "test:file": "export BABEL_DISABLE_CACHE=true && npx mocha --require \"babelRegister.js\"",
    "test": "export BABEL_DISABLE_CACHE=true && npx mocha --require \"babelRegister.js\" --require source-map-support \"./src/**/*.spec.ts\" --includeStack",
    "test:watch": "npx nodemon --exec \"npm test || true\"",
    "lint": "npx eslint src/ --ext .ts",
    "jsdoc": "npx jsdoc -c jsdoc.js",
    "build:ts": "export BABEL_FOR_TYPESCRIPT=true && rm -rf typescript/src && npx babel src --extensions \".ts\" -d typescript/src --keep-file-extension",
    "check:ts": "tsc -p typescript",
    "ts": "npm run build:ts && npm run check:ts",
    "check-all": "npm run lint; npm run test --silent; npm run ts --silent"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-do-expressions": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-proposal-pipeline-operator": "^7.8.3",
    "@babel/plugin-proposal-private-methods": "^7.8.3",
    "@babel/plugin-syntax-typescript": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/plugin-transform-typescript": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.8",
    "@types/ramda": "^0.26.39",
    "babel-eslint": "^11.0.0-beta.2",
    "babel-plugin-macros": "^2.6.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^6.8.0",
    "husky": "^4.0.10",
    "jsdoc": "^3.6.3",
    "mocha": "^7.0.0",
    "nodemon": "^2.0.2",
    "typedoc": "^0.16.7",
    "typescript": "^3.7.5"
  }
}
